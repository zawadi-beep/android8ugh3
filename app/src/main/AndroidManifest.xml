        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Android"
        tools:targetApi="31">
        <activity
            android:name=".splashactivity"
            android:exported="false"
            android:label="@string/title_activity_splashactivity"
            android:theme="@style/Theme.Android" />
        <activity
            android:name=".Lottiactivity"
            android:exported="false"
            android:label="@string/title_activity_lottiactivity"
            android:theme="@style/Theme.Android" />

        <activity
            android:name=".woofactivity"
            android:exported="false"
            android:label="@string/title_activity_woofactivity"
            android:theme="@style/Theme.Android" />
        <activity
            android:name=".Exploreactivity"
            android:exported="false"
            android:label="@string/title_activity_exploreactivity"
            android:theme="@style/Theme.Android" />
        <activity
            android:name=".Destinationactivity"
            android:exported="false"
            android:label="@string/title_activity_destinationactivity"
            android:theme="@style/Theme.Android" />
        <activity
            android:name=".formactivity"
            android:exported="false"
            android:label="@string/title_activity_formactivity"
            android:theme="@style/Theme.Android" />
        <activity
            android:name=".intentactivity"
            android:exported="false"
            android:label="@string/title_activity_intentactivity"
            android:theme="@style/Theme.Android" />
        <activity
            android:name=".Layoutactivity"
            android:exported="true"
            android:label="@string/title_activity_layoutactivity"
            android:theme="@style/Theme.Android">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Android">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>